<html>
    <head>
        <%- include( 'partials/head.ejs' ) %>
        <link rel="stylesheet" href="/styles/index.css">
    </head>
    <nav><%- include( 'partials/nav.ejs' )  %>
    </nav>
    <title>Keycaps</title>
    <body>
<h1>Keycap Sets</h1>
<% keycaps.forEach(function(keycap) { %>
    <div class="card" style="width: 18rem;">
        <img src="<%= keycap.image %>" max width="300px" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title"><%= keycap.name %></h5>
          <p class="card-text">$<%= keycap.price %></p>
          <a href="/products/keycaps/<%=keycap._id%>" class="btn btn-primary">View</a>
</div>
</div>
<% }); %>
</body>
<footer>
    <%- include( 'partials/footer.ejs' ) %>
    </footer>