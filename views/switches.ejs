<html>
    <head>
        <%- include( 'partials/head.ejs' ) %>
        <link rel="stylesheet" href="/styles/index.css">
    </head>
    <body>
    <nav><%- include( 'partials/nav.ejs' )  %>
    </nav>
    <title>Switches</title>
<h1 class="title">Switches</h1>
<% switches.forEach(function(keyswitch) { %>
    <div class="card" style="width: 18rem;">
        <img src="<%= keyswitch.image %>" max width="300px" max height="250px" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title"><%= keyswitch.name %></h5>
          <p class="card-text">$<%= keyswitch.price %></p>
          <a href="/products/switches/<%=keyswitch._id%>" class="btn btn-primary"><strong>View</strong></a>
        </div>
    </div>
<% }); %>
</body>
<footer>
    <%- include( 'partials/footer.ejs' ) %>
    </footer>